<p>Preferred Intel Processor?</p>
<form action='tech' method='post'>
  <ul class='unstyled'>
    <% @options.each do |id, text| %>
      <li>
        <label class='radio'>
          <input type='radio' name='intel' value='<%= id %>' id='intel_<%= id %>' />
          <%= text %>
        </label>
      </li>
    <% end %>
  </ul>
  <button type='submit' class='btn btn-primary'>Cast this vote!</button>
</form>
